{
  "extends": [],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "./node_modules/**/*.ts",
      "node_modules/**"
    ]
  },
  "rules": {
    "no-implicit-dependencies": [true, ["containers"]],
    "no-submodule-imports":[true,"containers"]
  }
}
